package Employee;

public class Employee {

	Node header = null;
	
	Employee(String name, float salary, int age) {
		Node node = new Node();
		node.setName(name);
		node.getName();
		node.setSalary(salary);
		node.getSalary();
		node.setAge(age);
		node.getAge();
		System.out.println(node.name);
	}

	public static void main(String arg[]) {
		Employee emp = new Employee("Ram",224000,125);
		
	}
}
