import java.util.*;

public class DetectLoop {

	Node header = null;

	DetectLoop() {

	}

	public void createLinkedList(int[] list) {

		for (int index = 0; index < list.length; index++) {
			Node node = new Node();

			node.data = list[index];
			node.next = null;

			if (header == null) {
				header = node;
			} else {
				node.next = header;
				header = node;
			}
		}
		node
	}

	boolean loopDetector() {
		boolean status = false;

		return status;
	}
}
