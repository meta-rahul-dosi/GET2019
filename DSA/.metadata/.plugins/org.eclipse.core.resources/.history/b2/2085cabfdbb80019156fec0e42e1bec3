package Employee;

public class Employee {

	void createLinkedList() {
		Node node = new Node();

		if (header == null)
			header = name;
	}

	Employee(String name, float salary, int age) {
		Node node = new Node();
		node.setName(name);
		node.getName();
		node.setSalary(salary);
		node.getSalary();
		node.setAge(age);
		node.getAge();
		
	}

	public static void main(String arg[]) {
		Employee emp = new Employee();
	}
}
