package Chemistry;

public class MoleculeOperation {

	Node root = null;

	void root()
	{
		Node node = new Node();	
		node.element = compound;
 		root = node;
	}
	
	void formTree(String compound)
	{
		if(root == null)
		{
     		Node node = new Node();	
			node.element = compound;
     		root = node;
		}
	}
}
