import java.util.*;

public class DetectLoop {

	Node header = null;

	DetectLoop() {

	}

	public void createLinkedList(int[] list) {

		for (int index = 0; index < list.length; index++) {
			Node node = new Node();
			Node node1 = new Node();
		
			node.data = inputList[index];
			node.next = null;
			if (header == null) {
				header = node;
			} else {
				node1 = header;
			}
			while (node1.next != null) {
				node1 = node1.next;
			}
			node1.next = node;
		}
			
		}

	}

	boolean loopDetector() {
		boolean status = false;

		return status;
	}
}
