	Node header = null;

	public static void main(String argument[]) {
		
	}

	 void Polynomial() {
		Node node = new Node();
		int index = 0;
		
		
		
		node.coefficient = 3;
		
		node.variables.add('x');
		node.variables.add('y');
		node.variables.add('y');
		
		node.powers.add(2);
		node.powers.add(2);
		
		node.next=null;
		
		
		if (header == null)
			header = node;
	}

	int returnDegree() {
		int degree=7;
		
		return degree;
	}

}
