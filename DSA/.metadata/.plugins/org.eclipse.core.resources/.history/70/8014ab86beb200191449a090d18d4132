import java.util.Scanner;

public class Polynomial {

	Scanner scanner = new Scanner(System.in);
	Node header = null;

	public static void main(String argument[]) {

	}

	void Polynomial() {
		Node node = new Node();

		node.coefficient = 3;
		node.variables.add('x');
		node.variables.add('y');
		node.variables.add('y');
		node.powers.add(2);
		node.powers.add(2);
		node.next = null;

		if (header == null)
			header = node;
		else
		{
			node.next = header;
			header = node;
		}
	}

	int returnDegree() {
		int degree = 7;

		return degree;
	}

}
