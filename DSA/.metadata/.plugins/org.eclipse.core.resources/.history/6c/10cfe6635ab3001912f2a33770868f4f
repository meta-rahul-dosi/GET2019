			node = node.next;
		sublistHeader = node;
		index = leftEnd;
		while (index++ <= rightEnd) {
			array[arrayIndex++] = node.data;
			System.out.println(node.data);
			node = node.next;
		}
		sublistFooter = node;
		return array;
	}

