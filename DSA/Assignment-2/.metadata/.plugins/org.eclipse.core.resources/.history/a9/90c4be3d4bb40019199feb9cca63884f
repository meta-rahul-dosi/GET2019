
public class QueueOperations implements Queue{

	private int[] queueStore = new int[100];
	private int front=0, rear=0;
	
	public static void main(String[] args) {
		
		add(5);
		add(6);
		add(7);
		delete();
	}
	
	void add(data)
	{
		rear = queueIndex;
		queueStore[queueIndex++] = data;
		rear = queueIndex;
	}

}
